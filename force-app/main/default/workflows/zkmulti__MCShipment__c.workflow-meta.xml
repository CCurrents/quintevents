<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>zkmulti__SendShipmentLabel</fullName>
        <description>SendShipmentLabel</description>
        <protected>false</protected>
        <recipients>
            <recipient>jpatel@quintevents.com</recipient>
            <type>user</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>zkmulti__ZenKraft_MultiCarrier/zkmulti__Empty_Template</template>
    </alerts>
    <outboundMessages>
        <fullName>Sf_Api_for_Shipments</fullName>
        <apiVersion>43.0</apiVersion>
        <endpointUrl>https://sf-api.quintevents.com/shipments/sf_update</endpointUrl>
        <fields>Id</fields>
        <fields>LastModifiedDate</fields>
        <fields>Opportunity__c</fields>
        <fields>Order__c</fields>
        <fields>PriceBook__c</fields>
        <fields>zkmulti__Carrier__c</fields>
        <fields>zkmulti__Status_Description__c</fields>
        <fields>zkmulti__Tracking_Number__c</fields>
        <fields>zkmulti__Tracking_Stage__c</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>qescobar@quintevents.com</integrationUser>
        <name>Sf Api for Shipments</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>QNG Shipments</fullName>
        <actions>
            <name>Sf_Api_for_Shipments</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>NOT($Setup.ByPass_Rules__c.Outbound_Messages__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>zkmulti__SendShipmentEmail</fullName>
        <active>false</active>
        <criteriaItems>
            <field>zkmulti__MCShipment__c.zkmulti__Label_Image_Type__c</field>
            <operation>contains</operation>
            <value>PNG</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
