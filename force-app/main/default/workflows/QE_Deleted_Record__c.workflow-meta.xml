<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>Sf_Api_for_Deleted_Records</fullName>
        <apiVersion>40.0</apiVersion>
        <description>This outbound message is sent when event, product or package products are deleted.</description>
        <endpointUrl>https://sf-api.quintevents.com/sf_delete</endpointUrl>
        <fields>DeletedId__c</fields>
        <fields>Id</fields>
        <fields>LastModifiedDate</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>jpatel@quintevents.com</integrationUser>
        <name>Sf Api for Deleted Records</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>QE_Deleted_Record</fullName>
        <actions>
            <name>Sf_Api_for_Deleted_Records</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>NOT($Setup.ByPass_Rules__c.Outbound_Messages__c)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
