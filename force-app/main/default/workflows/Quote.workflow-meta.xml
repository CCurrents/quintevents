    <fieldUpdates>
        <fullName>Draft_Awaiting_Approval_From</fullName>
        <description>Sets value for &quot;Awaiting Approval From&quot; field on quote once approval process starts.</description>
        <field>Awaiting_Approval_From__c</field>
        <formula>IF( 
Draft_Reasons_For_Review_H4H__c != &apos;&apos;, 
&quot;H4H&quot;, 
IF(AND(Owner:User.UserRole.Name = &apos;Exclusive GP&apos; ,Draft_Reasons_For_Review_Sales__c != &apos;&apos;), 
&quot;Exclusive GP&quot;, 
IF(AND(Owner:User.UserRole.Name != &apos;Exclusive GP&apos; ,Draft_Reasons_For_Review_Sales__c != &apos;&apos;), 
&quot;Sales&quot;, 
IF(Draft_Reasons_For_Review_Operations__c != &apos;&apos;, 
&quot;Operations&quot;, 
IF(Draft_Reasons_For_Review_Corporate_Sales__c != &apos;&apos;, 
&quot;Corporate Sales&quot;, 
&quot;&quot;) 
) 
) 
) 
)</formula>
        <name>Draft Awaiting Approval From</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Draft_Awaiting_Approval_From_H4H</fullName>
        <field>Awaiting_Approval_From__c</field>
        <formula>if(AND(Owner:User.UserRole.Name != &apos;Exclusive GP&apos; ,Draft_Reasons_For_Review_Sales__c != &apos;&apos;), 
&quot;Sales&quot;, 
if(AND(Owner:User.UserRole.Name = &apos;Exclusive GP&apos; ,Draft_Reasons_For_Review_Sales__c != &apos;&apos;), 
&quot;Exclusive GP&quot;, 
if(Draft_Reasons_For_Review_Operations__c != &apos;&apos;, 
&quot;Operations&quot;, 
if(Draft_Reasons_For_Review_Corporate_Sales__c != &apos;&apos;, 
&quot;Corporate Sales&quot;, 
&quot;&quot;) 
) 
) 
)</formula>
        <name>Draft Awaiting Approval From H4H</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
