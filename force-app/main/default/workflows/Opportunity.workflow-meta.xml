    <rules>
        <fullName>QNG Opportunity</fullName>
        <actions>
            <name>Sf_Api_for_Opportunity</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <description>Sends Outbounds Message to SF API for Opportunity</description>
        <formula>IF(AND(NOT($Setup.ByPass_Rules__c.Outbound_Messages__c), Refund_Reconciliation__c = false, OR(Pricebook2.GPE_Event__c = false, GPE_Online_Order__c = true)), true, false)</formula>
