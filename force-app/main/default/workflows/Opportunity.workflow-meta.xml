    <rules>
        <fullName>QNG Opportunity Payment Emails</fullName>
        <actions>
            <name>QNG_Opportunity_Payment_Emails</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>if(AND(NOT($Setup.ByPass_Rules__c.Outbound_Messages__c), ISPICKVAL(StageName , &apos;Closed Won&apos;), ISPICKVAL(Payment_Method__c , &apos;Credit Card&apos;), Send_Payment_Email__c = true, Online_Order__c = false, OR(Pricebook2.GPE_Event__c = false, GPE_Online_Order__c = true), CloseDate &gt;= DATE(2020,9,8)),true, false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>QNG Opportunity Resend Payment Emails</fullName>
        <actions>
            <name>QNG_Opportunity_Payment_Emails</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <description>Removed &quot;&amp;&amp; (Datevalue(NOW()) - (CloseDate)  &lt;= 2)&quot; as this workflow is related to resend payment email functionality.</description>
        <formula>IF(AND( NOT($Setup.ByPass_Rules__c.Outbound_Messages__c), Send_Outbound_Message__c = true,   ISPICKVAL(StageName , &apos;Closed Won&apos;), OR(Pricebook2.GPE_Event__c = false, GPE_Online_Order__c = true),  Refund_Reconciliation__c = false), true, false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>QE_Opportunity_Send_Outbound_Message</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Opportunity.LastModifiedDate</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
