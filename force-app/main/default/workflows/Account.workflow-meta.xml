    <rules>
        <fullName>Update Account Event Interests based on Latest</fullName>
        <actions>
            <name>update_Event_Interest_with_Latest</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>update Account latest event interests based on Latest Event Interest</description>
        <formula>OR(ISNEW(),ISCHANGED(Latest_Event_Interest__c))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Account Latest Event Interests</fullName>
        <actions>
            <name>update_Account_Latest_Event_Interest</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>update Account latest event interests based on Picklist</description>
        <formula>AND(NOT(ISBLANK( Event_Interest_Picklist_PB__c )), OR(ISNEW(),ISCHANGED(Event_Interest_Picklist_PB__c)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Person Account Event Interests based on Latest</fullName>
        <actions>
            <name>update_Event_Interest_with_Latest_P</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>update Person Account latest event interests based on Latest Event Interest</description>
        <formula>OR(ISNEW(),ISCHANGED(Latest_Event_Interest__pc)) &amp;&amp;  IsPersonAccount = True</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Person Account Latest Event Interests</fullName>
        <actions>
            <name>update_Account_Latest_Event_Interest_P</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>update Person Account latest event interests based on Picklist</description>
        <formula>AND(NOT(ISBLANK(Event_Interest_Picklist_PB__pc)), OR(ISNEW(),ISCHANGED(Event_Interest_Picklist_PB__pc))) &amp;&amp;
IsPersonAccount = true</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>update Person Account Event Interests</fullName>
        <actions>
            <name>update_Account_Event_Interests</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>update_Acct_Latest_Event_Int_with_Pickli</name>
