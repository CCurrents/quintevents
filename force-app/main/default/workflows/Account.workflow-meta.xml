    <rules>
        <fullName>Account Latest Event Status</fullName>
        <actions>
            <name>Update_Deposit_Request_Events</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>(
(ISNEW() &amp;&amp; LOWER(Latest_Event_Status__c) = &apos;deposit&apos;) ||
(ISCHANGED(Latest_Event_Status__c) &amp;&amp; LOWER(Latest_Event_Status__c) = &apos;deposit&apos;)
)
||
((ISCHANGED(Event_Interest_Picklist__c) || ISCHANGED(Latest_Event_Interest__c)) &amp;&amp; LOWER(Latest_Event_Status__c) = &apos;deposit&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Accounts to QNG</fullName>
        <actions>
            <name>Accounts_to_QNG</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <description>Sends Outbounds Message to QNG for Account</description>
        <formula>NOT($Setup.ByPass_Rules__c.Outbound_Messages__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>F1 Buyer Roll Up Count</fullName>
        <actions>
            <name>update_F1_Buyer_Count</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Account.F1_Buyer_Roll_Up__c</field>
            <operation>notEqual</operation>
            <value>0</value>
        </criteriaItems>
        <description>update the F1 Buyer field with F1 Buyer Roll Up Count for Account Sharing purposes

i.e.  Share F1 Accounts with Charlotte Hill per MC

-8/17/18</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
